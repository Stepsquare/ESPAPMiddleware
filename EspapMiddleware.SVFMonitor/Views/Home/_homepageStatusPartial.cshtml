@model HomepageStatusPartialViewModel

<div class="row">
        <div class="col-md-2">
            <h5 class="ig-dash-title">TOTAL DOCUMENTOS:</h5>
            <h4>@Model.TotalDocuments documentos</h4>
        </div>
        <div class="col-md-2">
            <h5 class="ig-dash-title">TOTAL DOCUMENTOS VALIDADOS:</h5>
            <h4>@Model.TotalValidDocuments documentos</h4>
        </div>
        <div class="col-md-2">
            <h5 class="ig-dash-title">TOTAL PAGOS:</h5>
            <h4>@Model.TotalPaidDocuments documentos</h4>
        </div>
        <div class="col-md-3">
            <h5 class="ig-dash-title">TOTAL DOCUMENTOS INVALIDADOS:</h5>
            <h4>@Model.TotalInvalidDocuments documentos</h4>
        </div>
        <div class="col-md-3">
            <h5 class="ig-dash-title">TOTAL DOCUMENTOS INVALIDADOS REGULARIZADOS:</h5>
            <h4>@Model.TotalInvalidDocumentsRectified documentos</h4>
        </div>
</div>


